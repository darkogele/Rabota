<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="NumberOfRequests" value="150"/>
    <add key="CallMethodGetM1M2" value="true"/>
    <add key="CallMethodGetM1M2ById" value="false"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="M1M2ServiceSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="M1M2ServiceSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://externalhandler-akn/MIM2$$AVRM/m1m2"
                binding="customBinding" bindingConfiguration="M1M2ServiceSoap12"
                contract="M1M2ServiceRef.M1M2ServiceSoap" name="M1M2ServiceSoap12" />
        </client>
    </system.serviceModel>
</configuration>