    <add name='connectionStringCC' connectionString='Data Source=.;User Id=sa;Password=E1da1ch1;Initial Catalog=CCAKN;' providerName='System.Data.SqlClient' xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
  </connectionStrings>
  <system.web>
