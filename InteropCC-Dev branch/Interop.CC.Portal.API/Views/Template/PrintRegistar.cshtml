            {
                <td>@prostor.Stavka</td>
            }
        </tr>
        <tr>
            <th>Општина</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Opstina</td>
            }
        </tr>
        <tr>

            <th>Назив на општина</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Naziv_opstina</td>
            }
        </tr>
        <tr>
            <th>Место</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Mesto</td>
            }
        </tr>
        <tr>
            <th>Назив на Место</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Naziv_mesto</td>
            }
        </tr>
        <tr>
            <th>Назив на улица</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Naziv_ulica</td>
            }
        </tr>
        <tr>
            <th>Улица број</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Ulica_broj</td>
            }
        </tr>
        <tr>
            <th>За производство</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Za_proizvodstvo</td>
            }
        </tr>
        <tr>
            <th>За складирање</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Za_skladiranje</td>
            }
        </tr>
        <tr>
            <th>ЕМБГ на одговорно лице</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Embg_odgovorno_lice</td>
            }
        </tr>
        <tr>
            <th>Назив на одговорно лице</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Naziv_odgovorno_lice</td>
            }
        </tr>
        <tr>
            <th>Забелешка</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Zabeleska</td>
            }
        </tr>
        <tr>
            <th>Статус</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Status</td>
            }
        </tr>
        <tr>
            <th>Статус датум</th>
            @foreach (var prostor in Model.ExciseSpaces)
            {
                <td>@prostor.Status_datum</td>
            }
        </tr>

